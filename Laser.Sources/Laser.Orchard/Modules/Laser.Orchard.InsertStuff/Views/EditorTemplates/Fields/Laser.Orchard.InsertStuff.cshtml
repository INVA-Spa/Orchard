@using Laser.Orchard.InsertStuff.Settings
@model Laser.Orchard.InsertStuff.ViewModels.InsertStuffViewModel

@{
    Script.Require("jQuery").AtFoot();
    Script.Include("jquery.validate.min.js");

    Model.StyleList.ForEach(x => Style.Include(x).AtHead());
    Model.ScriptList.ForEach(x => Script.Include(x).AtFoot());

}
@if (Model.OnFooter) {
    using (Script.Foot()) {
    @Html.Raw(Model.RawHtml)
    }
}
else {
    @Html.Raw(Model.RawHtml)
}