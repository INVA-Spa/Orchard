@{
    dynamic item = Model.ContentItem;

    var medias = item.ShowcaseImmaginiWidget.Gallery.MediaParts;
}

<div class="row">

    @for (int i = 0; i < medias.Count; i++) {
        <div class="col-xs-12 col-sm-6 text-center">
		    <img src="@medias[i].MediaUrl" class="img-responsive">
	    </div>
    }

</div>