msgid "Core"
msgstr "Core"

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "Site name is required."
msgstr "Nome del sito è richiesto."

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "Site name can be no longer than {0} characters."
msgstr "Il nome del sito non può essere più lungo di {0} caratteri."

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "User name is required."
msgstr "Il nome utente è obbligatorio."

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "User name must be at least {0} characters."
msgstr "Nome utente deve essere almeno {0} caratteri."

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "User name can be no longer than {0} characters."
msgstr "Il nome utente non può essere più lungo di {0} caratteri."

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "Password is required."
msgstr "La password è obbligatoria."

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "Password must be at least {0} characters."
msgstr "Password deve essere almeno {0} caratteri."

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "Password can be no longer than {0} characters."
msgstr "La password non può essere più lunga di {0} caratteri."

#: "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgctxt "Orchard.Setup.Annotations.SiteNameValidAttribute"
msgid "Password confirmation is required."
msgstr "La conferma della password è obbligatoria."

#: "Orchard.Setup.Commands.SetupCommand"
msgctxt "Orchard.Setup.Commands.SetupCommand"
msgid "Setup of site '{0}' was started with recipe execution ID {1}. Use the 'recipes result' command to check the result of the execution."
msgstr "L'installazione del sito '{0}' è stato avviato con l' esecuzione della ricetta con ID {1}. Utilizzare il comando 'risultato ricette' per verificare il risultato dell'esecuzione."

#: "Orchard.Setup.Controllers.SetupController"
msgctxt "Orchard.Setup.Controllers.SetupController"
msgid "A connection string is required."
msgstr "È necessaria una stringa di connessione."

#: "Orchard.Setup.Controllers.SetupController"
msgctxt "Orchard.Setup.Controllers.SetupController"
msgid "Password confirmation must match."
msgstr "Le due password non combaciano."

#: "Orchard.Setup.Controllers.SetupController"
msgctxt "Orchard.Setup.Controllers.SetupController"
msgid "The table prefix must begin with a letter."
msgstr "Il prefisso per le tabelle deve iniziare con una lettera."

#: "Orchard.Setup.Controllers.SetupController"
msgctxt "Orchard.Setup.Controllers.SetupController"
msgid "The table prefix must contain letters or digits."
msgstr "Il prefisso per le tabelle deve contenere solo lettere o numeri."

#: "Orchard.Setup.Controllers.SetupController"
msgctxt "Orchard.Setup.Controllers.SetupController"
msgid "No recipes were found."
msgstr "Sono state trovate ricette."

#: "Orchard.Setup.Controllers.SetupController"
msgctxt "Orchard.Setup.Controllers.SetupController"
msgid "Setup failed: {0}"
msgstr "Configurazione non riuscita: {0}"

#: "Orchard.Setup.Services.SetupService"
msgctxt "Orchard.Setup.Services.SetupService"
msgid "A previous Orchard installation was detected in this database with this table prefix."
msgstr "È presente una precedente installazione di Orchard che utilizza lo stesso prefisso di tabella nel medesimo database."

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Get Started"
msgstr "Per iniziare"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Please answer a few questions to configure your site."
msgstr "Rispondi ad un paio di domande per configurare il tuo sito."

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "What is the name of your site?"
msgstr "Qual'é il nome del tuo sito?"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Choose a user name:"
msgstr "Scegli un nome utente:"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Choose a password:"
msgstr "Scegli una password:"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Confirm the password:"
msgstr "Conferma la password:"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "How would you like to store your data?"
msgstr "Come vuoi salvare i tuoi dati?"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Use built-in data storage (SQL Server Compact)"
msgstr "Usa l'archivio integrato (SQL Server Compact)"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Use an existing SQL Server, SQL Express database"
msgstr "Utilizza un database SQL Server o SQL Express pre-esistente"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Use an existing MySql database"
msgstr "Utilizza un database MySQL pre-esistente"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Use an existing PostgreSQL database"
msgstr "Utilizzare un database esistente di PostgreSQL"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Connection string"
msgstr "Stringa di connessione"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Data Source=sqlServerName;Initial Catalog=dbName;User ID=userName;Password=password"
msgstr "Data Source = sqlServerName; Initial Catalog = dbName; User ID = userName; Password = password"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Data Source=serverName;Database=dbName;User Id=userName;Password=password"
msgstr "Data Source=nomeServer;Database=nomeDatabase;User Id=nomeUtente;Password=password"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Server=serverName;Port=5432;Database=dbName;User Id=userName;Password=password"
msgstr "Server = serverName; Porta = 5432; Database = dbName; User Id = userName; Password = password"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Database Table Prefix"
msgstr "Prefisso delle tabelle del database"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Choose an Orchard Recipe"
msgstr "Scegli una Ricetta di Orchard"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Orchard Recipes allow you to setup your site with additional pre-configured options, features and settings out of the box."
msgstr "Le Ricette di Orchard rendono possibile creare il tuo sito con una serie di opzioni, funzionalità ed impostazioni pre-configurate."

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Unspecified"
msgstr "Non specificato"

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Cooking Orchard Recipe ..."
msgstr "Ricetta di Orchard in fase di cottura ..."

#: "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgctxt "~/Modules/Orchard.Setup/Views/Setup/Index.cshtml"
msgid "Finish Setup"
msgstr "Concludi l'installazione"

